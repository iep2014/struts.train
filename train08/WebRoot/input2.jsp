<%@ page language="java" contentType="text/html; charset=GBK"%>
<html>
<head>
<title>类型转换器</title>
</head>
<body>
<form action="login2.action" method="post">
    <table align="center" width="360">
    <caption><h3>类型转换器</h3></caption>
        <tr>
            <td>用户：<input type="text" name="user.name"/></td>
        </tr>
        <tr>
            <td>密码：<input type="password" name="user.pass"/></td>
        </tr>
        <tr align="center">
            <td><input type="submit" value="转换"/><input type="reset" value="重填"/></td>
        </tr>
    </table>
</form>
</body>
</html>
